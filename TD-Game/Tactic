MATCH (alien_strategy:Policy_Alien)
MATCH (alien_strategy)<-[rel]-(human_strategy:Policy_Human)
WITH type(rel) AS t, collect( DISTINCT human_strategy) AS strategies, count( DISTINCT alien_strategy) AS alien_counter, 
	REDUCE(uniqueCodes=[], 
         x in COLLECT(human_strategy) |
         CASE WHEN x IN uniqueCodes THEN uniqueCodes ELSE uniqueCodes+x END
   ) AS codes
RETURN t, codes, strategies, alien_counter